import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        if(a<10 && a>0){
        for(int i=1;i<=a;i++){
            System.out.println(a + " x " + i +" = " + (a*i));
        }
        }else{
                System.out.println("Invalid Input");
        }
        
    }
}
