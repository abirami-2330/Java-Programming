import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int countpos=0;
        int countneg=0;
        
        for(int i=0;i<num;i++)
        {
           int a=sc.nextInt();
            if(a>0)
            {
                countpos++;
            }
            else if(a<0)
            {
                    countneg++;
                    
                }
        }
            
            System.out.printf("Count of Positive Integer is %.2f\n", (float)countpos);
                System.out.printf("Count of Negative Integer is %.2f\n", (float)countneg);

        }
        
    
}
